{
  "collectionName": "components_variaciones_variaciones",
  "info": {
    "displayName": "Variaciones",
    "description": ""
  },
  "options": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "sku": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "regularPrice": {
      "type": "integer"
    },
    "stockStatus": {
      "type": "enumeration",
      "enum": [
        "En Existencia",
        "Agotado"
      ]
    },
    "state": {
      "type": "enumeration",
      "default": "Activo",
      "enum": [
        "Activo",
        "Desactivado"
      ]
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "attribute": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::attribute.attribute"
    },
    "manageStock": {
      "type": "boolean",
      "default": false
    },
    "stockQty": {
      "type": "integer",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "manageStock"
            },
            true
          ]
        }
      }
    },
    "lowStockAmount": {
      "type": "integer",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "manageStock"
            },
            true
          ]
        }
      }
    }
  }
}
