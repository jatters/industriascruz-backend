{
  "kind": "collectionType",
  "collectionName": "departments",
  "info": {
    "singularName": "department",
    "pluralName": "departments",
    "displayName": "Departamentos"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "wooId": {
      "type": "integer"
    },
    "cities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::city.city",
      "mappedBy": "department"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "shipping_zone": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::shipping-zone.shipping-zone",
      "inversedBy": "departments"
    }
  }
}
