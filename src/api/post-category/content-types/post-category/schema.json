{
  "kind": "collectionType",
  "collectionName": "post_categories",
  "info": {
    "singularName": "post-category",
    "pluralName": "post-categories",
    "displayName": "Categor√≠a de Articulo"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "blocks"
    },
    "SEO": {
      "type": "component",
      "component": "seo.seo",
      "repeatable": false
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::articulo.articulo",
      "mappedBy": "category"
    }
  }
}
